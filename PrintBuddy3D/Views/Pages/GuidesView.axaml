<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:suki="https://github.com/kikipoulet/SukiUI"
             xmlns:vm="clr-namespace:PrintBuddy3D.ViewModels.Pages"
             x:Class="PrintBuddy3D.Views.Pages.GuidesView"
             x:DataType="vm:GuidesViewModel">
    <suki:GlassCard Margin="5">
        <suki:SukiSideMenu IsSearchEnabled="False" SearchText="{Binding SearchText}" ItemsSource="{Binding WikiPages}" />
    </suki:GlassCard>
</UserControl>