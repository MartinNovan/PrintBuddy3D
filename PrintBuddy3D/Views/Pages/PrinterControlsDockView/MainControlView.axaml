<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:printerControlsDockView="clr-namespace:PrintBuddy3D.Views.Pages.PrinterControlsDockView"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:DataType="printerControlsDockView:MainControlViewModel"
             x:Class="PrintBuddy3D.Views.Pages.PrinterControlsDockView.MainControlView">
    <Grid RowDefinitions="*,25">
        <TextBlock
            Text="{Binding FocusedDockable}"
            Margin="4"
            Grid.Row="1"/>
        <DockControl
            Grid.Row="0"
            Margin="4"
            Layout="{Binding ActiveDockable}" />
    </Grid>
</UserControl>
