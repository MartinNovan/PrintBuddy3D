<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="PrintBuddy3D.App"
             xmlns:suki="using:SukiUI"
             xmlns:material="using:Material.Icons.Avalonia"
             xmlns:common="clr-namespace:PrintBuddy3D.Common"
             xmlns:dockControls="using:Dock.Avalonia.Controls"
             RequestedThemeVariant="Dark">

    <Application.DataTemplates>
        <common:ViewLocator/>
    </Application.DataTemplates>
  
    <Application.Styles>
        <FluentTheme/> <!--Needed for the dock to render properly-->
        <StyleInclude Source="avares://SukiUI.Dock/Index.axaml" />
        <suki:SukiTheme ThemeColor="Blue"/>
        <material:MaterialIconStyles/>
    </Application.Styles>
</Application>